devicedb_conn_config:
    devicedb_uri: "https://{{DEVICE_ID}}:9090"
    devicedb_prefix: "vagrant"
    devicedb_bucket: "lww"
    relay_id: "{{DEVICE_ID}}"
    ca_chain: "{{DEVICEDB_SRC}}/hack/certs/myCA.pem"
targets:
    - file: "/var/log/maestro/maestro.log"
      format_time: "[%ld:%d] "
      filters:
      - levels: error
config_end: true
